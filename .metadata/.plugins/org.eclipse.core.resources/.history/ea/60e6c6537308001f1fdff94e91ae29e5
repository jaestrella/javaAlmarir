package ejemplo5;

import java.util.Scanner;

public class ejemplo5 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Introduce nombre del alumno: ");
		String nombre=sc.nextLine();
		System.out.println("Introduce asignatura: ");
		String asignatura=sc.nextLine();
		System.out.println("Introduce la nota: ");
		int nota=sc.nextInt();
		
		alumno a1=new alumno(nombre,asignatura,nota);
		
		if(nota<=4) {
			a1.setNota(Integer.parseInt("Insuficiente"));
		}else if(nota==5) {
			a1.setNota(Integer.parseInt("Suficiente"));
		}else if(nota==6) {
			a1.setNota(Integer.parseInt("Bien"));
		}else if(nota==7 || nota==8) {
			a1.setNota(Integer.parseInt("Notable"));
		}else {
			a1.setNota(Integer.parseInt("Sobresaliente"));
		}
		
		System.out.println(a1.toString());
	}

}
